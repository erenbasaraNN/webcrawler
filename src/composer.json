{
  "name": "my-web-scraper/project",
  "description": "A web scraping project using PHP and Symfony components",
  "type": "project",
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.4",
    "symfony/dom-crawler": "^5.3",
    "symfony/css-selector": "^5.3",
    "ext-dom": "*",
    "ext-simplexml": "*",
    "setasign/fpdi": "^2.3",
    "setasign/fpdf": "^1.8",
    "setasign/fpdi-tcpdf": "^2.0",
    "psr/log": "^1.1",
    "monolog/monolog": "^2.2"
  },

  "autoload": {
    "psr-4": {
      "App\\": "."
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "symfony/var-dumper": "^5.3"
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "minimum-stability": "stable",
  "license": "MIT"
}
